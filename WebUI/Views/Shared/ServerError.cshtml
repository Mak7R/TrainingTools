@model IEnumerable<string>

@{
    ViewBag.Title = $"{Context.Response.StatusCode} - Error while processing the request";
}

<h2 class="text-center">@Context.Response.StatusCode</h2>
<h3 class="text-center">Error while processing the request</h3>
<div class="container align-content-center">
    @foreach (var error in Model)
    {
        <p class="text-warning text-center">@error</p>
    }
</div>


